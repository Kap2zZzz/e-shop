@model IEnumerable<e_shop.Entities.AskClient>

<div class="panel panel-default">
    <div class="panel-heading"><h4>Усі питання</h4></div>
    <div class="panel-body">
        <table class="table table-striped table-condensed table-bordered">
            <thead>
                <tr>
                    <th class="text-right">ID</th>
                    <th class="text-center">Дата створення</th>
                    <th class="text-left">Ім’я</th>
                    <th class="text-center">Телефон</th>
                    <th class="text-center">E-mail</th>
                    <th class="text-center">Тема</th>
                    <th class="text-center">Питання</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="text-right">@item.AskClientID</td>
                        <td class="text-center">@item.CreateDateTime</td>
                        <td class="text-left">@item.Name</td>
                        <td class="text-center">@item.Phone</td>
                        <td class="text-center">@item.Email</td>
                        <td class="text-center">@item.Theme</td>
                        <td class="text-center">@item.Question</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
